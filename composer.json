{
    "name": "as-software/laravel",
    "description": "An Laravel Extension for increasing your coding speed.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AaronSlots",
            "email": "aaronslots@gmail.com"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "simple"
    ],
    "require": {
        "php": "^7.3"
    },
    "autoload-dev": {
        "psr-4": {
            "ASSoftware\\Laravel\\":"packages/as-software/laravel/src",
            "App\\": "app/"
        },
        "classmap": ["src"]
    },
	"extra": {
		"laravel": {
			"providers": [
				"ASSoftware\\Laravel\\LaravelServiceProvider"
			]
		}
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
	"minimum-stability": "dev"
}
